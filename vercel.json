{
  "version": 2,
  "name": "story-teller",
  "buildCommand": "cd Frontend && npm run build",
  "outputDirectory": "Frontend/dist",
  "installCommand": "cd Frontend && npm install",
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/docs",
      "dest": "/api/index.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/api/index.py"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/Frontend/assets/$1"
    },
    {
      "src": "/(.*\\..+)",
      "dest": "/Frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/index.html"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "regions": ["iad1"]
}
